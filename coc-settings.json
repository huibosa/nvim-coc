{
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": ["go", "python"],
  "coc.preferences.jumpCommand": "vsplit",

  "hover.floatConfig": {"border": true},
  "diagnostic.floatConfig": {"border": true},
  "signature.floatConfig": {"border": true},

  "diagnostic.warningSign": "▐",
  "diagnostic.errorSign": "▐",
  "diagnostic.infoSign": "▐",
  "diagnostic.hintSign": "▐",

  "suggest.noselect": false,
  "suggest.enablePreselect": true,
  "suggest.enablePreview": true,
  "suggest.triggerCompletionWait": 30,
  "suggest.floatConfig": {"border": true},
  "suggest.asciiCharactersOnly": true,

  "snippets.snipmate.enable": false,
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.directories": [ "UltiSnips" ],
  "snippets.loadFromExtensions": false,
  "snippets.extends": {"latex": ["tex", "texmath"]},

  "yank.enableCompletion": false,

  "clangd.path": "~/.local/bin/clangd_13.0.0/bin/clangd",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/usr/bin/black",
  "go.goplsOptions": { "staticcheck": true },
  "go.goplsPath": "gopls",
  "Lua.telemetry.enable": false,
  "sumneko-lua.serverDir": "/home/huibosa/.local/bin/lua-language-server"
}
